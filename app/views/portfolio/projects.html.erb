<div class="pusher">
<h1>favorite</h1>
</div>

<div class="projects">
	<ul class="flex-container">
	  <li class="flex-item">Portfolio</li>
	  <li class="flex-item">Screenshot</li>
	  <li class="flex-item">Description</li>
	  <li class="flex-item">
		<canvas class="graph" id="portfolio_graph">
		<script>
			$(document).ready(function(){
				var ctx = $("#portfolio_graph").get(0).getContext("2d");

				var data = [
					{
						value: <%= @portfolio[1] %>,
						color: "#563D7C",
						highlight: "purple",
						label: "CSS"
					},
					{
						value: <%= @portfolio[3] %>,
						color: "brown",
						highlight: "brown",
						label: "CoffeeScript"
					},
					{
						value: <%= @portfolio[5] %>,
						color: "#E44B23",
						highlight: "orange",
						label: "HTML"
					},
					{
						value: <%= @portfolio[7] %>,
						color: "#F1E05A",
						highlight: "yellow",
						label: "JavaScript"
					},
					{
						value: <%= @portfolio[9] %>,
						color: "#701516",
						highlight: "red",
						label: "Ruby"
					}
				];

				//draw
				var piechart = new Chart(ctx).Pie(data);
			});
			</script>
		</canvas>
		</li>
	  <li class="flex-item">Links</li>
	</ul>
</div>

